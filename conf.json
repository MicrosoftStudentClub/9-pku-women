{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        },
        "systemName"      : "himins",
        "footer"          : "",
        "copyright"       : "Copyright 2013-2015 John Pavley. MIT License.",
        "navType"         : "vertical",
        "theme"           : "SuperHero",
        "linenums"        : true,
        "collapseSymbols" : false,
        "inverseNav"      : true
    }
}